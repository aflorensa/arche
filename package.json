{
  "name": "archetyping-archetyper",
  "version": "1.4.0",
  "description": "this project aims to build seed projects from existing justdigital seeds",
  "main": "index.js",
  "scripts": {
    "precompile": "rimraf app/**/*.js test/**/*.js",
    "compile": "ntsc -p .",
    "pretest": "rimraf app/**/*.js test/**/*.js",
    "test": "nyc mocha",
    "start": "node dist/index.js"
  },
  "author": "xavi hidalgo",
  "homepage": "https://github.com/medeasolutions/archetyper",
  "private": true,
  "devDependencies": {
    "@types/chai": "~3.5.2",
    "@types/mocha": "~2.2.41",
    "@types/node": "^7.0.52",
    "@types/prompt-sync": "^4.1.0",
    "chai": "~3.5.0",
    "mocha": "~3.5.3",
    "nyc": "~11.0.3",
    "prompt-sync": "^4.1.5",
    "rimraf": "~2.5.2",
    "ts-node": "~3.1.0",
    "typescript": "~2.2.0",
    "es6-shim": "^0.35.3",
    "fs-extra": "^5.0.0",
    "inquirer": "^5.0.1",
    "replace-in-file": "^3.1.0",
    "shelljs": "^0.8.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.*",
      "test/resources/sampleLanguages"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "bin": {
    "archetyper": "./dist/index.js"
  },
  "dependencies": {
    "await": "^0.2.6",
    "chalk": "^1.1.3",
    "clear": "0.0.1",
    "clui": "^0.3.1",
    "figlet": "^1.1.2",
    "fs-extra-plus": "^0.1.3",
    "github": "^2.1.0",
    "inquirer": "^1.1.0",
    "lodash": "^4.13.1",
    "minimist": "^1.2.0",
    "preferences": "^0.2.1",
    "prompt-sync-history": "^1.0.1",
    "simple-git": "^1.40.0",
    "touch": "^1.0.0"
  }
}
