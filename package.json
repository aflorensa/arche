{
  "name": "archetyping-archetyper",
  "version": "1.4.0",
  "description": "this project aims to build seed projects from existing justdigital seeds",
  "main": "index.js",
  "scripts": {
    "precompile": "rimraf app/**/*.js test/**/*.js",
    "compile": "tsc",
    "pretest": "rimraf app/**/*.js test/**/*.js",
    "test": "nyc mocha"
  },
  "author": "xavi hidalgo",
  "homepage": "https://github.com/medeasolutions/archetyper",
  "private": true,
  "devDependencies": {
    "@types/chai": "~3.5.2",
    "@types/mocha": "~2.2.41",
    "@types/node": "^7.0.52",
    "chai": "~3.5.0",
    "fs-extra": "^5.0.0",
    "mocha": "~3.2.0",
    "nyc": "~11.0.3",
    "replace-in-file": "^3.1.0",
    "rimraf": "~2.5.2",
    "ts-node": "~3.1.0",
    "typescript": "~2.2.0"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/**/*.*",
      "test/resources/sampleLanguages"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html",
      "text"
    ]
  },
  "dependencies": {
    "es6-shim": "^0.35.3",
    "shelljs": "^0.8.1"
  }
}
